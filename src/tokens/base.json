{
  "color": {
    "base": {
      "black": { "$type": "color", "$value": "#0b0b0e" },
      "white": { "$type": "color", "$value": "#ffffff" },
      "smoke": { "$type": "color", "$value": "#141418" }
    },
    "monokai": {
      "pink": { "$type": "color", "$value": "#ff6188" },
      "orange": { "$type": "color", "$value": "#fb9866" },
      "yellow": { "$type": "color", "$value": "#ffd866" },
      "green": { "$type": "color", "$value": "#a9dc75" },
      "cyan": { "$type": "color", "$value": "#78dce8" },
      "purple": { "$type": "color", "$value": "#ab9df2" },
      "background": { "$type": "color", "$value": "#231f22" },
      "background-alt": { "$type": "color", "$value": "#2e2a2e" },
      "foreground": { "$type": "color", "$value": "#fcfcfa" }
    }
  },
  "font": {
    "mono": {
      "$type": "fontFamily",
      "$value": "MonoLisa, \"SF Mono\", Consolas, monospace"
    },
    "sans": {
      "$type": "fontFamily",
      "$value": "ui-sans-serif, system-ui, -apple-system, sans-serif"
    },
    "serif": {
      "$type": "fontFamily",
      "$value": "ui-serif, Georgia, serif"
    }
